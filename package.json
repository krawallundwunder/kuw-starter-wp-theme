{
  "name": "kuw-wp-theme",
  "version": "1.0.0",
  "repository": "https://github.com/krawallundwunder/pacemo-wp-theme/",
  "author": "krawall & wunder <info@krawallundwunder.de>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "bun run clean && vite dev",
    "clean": "rm -rf ./dist && mkdir -p ./dist",
    "build": "NODE_ENV=production vite build",
    "build:lint": "bun run lint && NODE_ENV=production vite build",
    "lint": "./vendor/bin/phpcs",
    "format": "./vendor/bin/phpcbf"
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
    "countup.js": "^2.8.0",
    "delegate-event-listener": "^1.2.0",
    "lazysizes": "^5.3.2",
    "rellax": "^1.12.1",
    "swiper": "^11.1.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.16",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "vite": "^5.2.11",
    "vite-plugin-full-reload": "^1.1.0"
  },
  "browserslist": [
    "supports array-flat"
  ]
}
