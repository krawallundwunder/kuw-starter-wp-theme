	<div class="container my-16 md:my-24" id="faq_section">
		<h2 class="font-sans text-3xl font-extrabold text-neutral-900">
			{{ module.title }}
		</h2>
		<div class="pt-10 ">
			<div class="space-y-10 md:space-y-0 md:grid md:grid-cols-2 md:gap-x-8 md:gap-y-12">
				{% for item in module.faq_section %}
					<div class="prose prose-a:text-primary-500">
						<p class="text-lg font-medium leading-6 text-primary-500">{{ item.question }}</p>
						<p class="mt-2 text-neutral-700">{{ item.answer }}</p>
					</div>
				{% endfor %}

			</div>
		</div>
	</div>
